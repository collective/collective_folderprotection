<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 
 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="assignpassword" meta_type="CMF Action"
     i18n:domain="collective_folderprotection">
   <property name="title" i18n:translate="">Assign password</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${plone_context_state/canonical_object_url}/@@assignpassword</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:plone_context_state.canonical_object().restrictedTraverse('@@authorized_to_set_password').allowed()</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>

 <object name="object_buttons" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="enable_password_protect" meta_type="CMF Action"
     i18n:domain="collective.folderprotection">
   <property name="title"
      i18n:translate="">Enable Password Protection</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${globals_view/getCurrentFolderUrl}/enable_password_protection</property>
   <property name="link_target"></property>
   <property
      name="icon_expr">string:/++resource++resources/lock_locked_16.png</property>
   <property
      name="available_expr">python:plone_context_state.canonical_object().restrictedTraverse('@@authorized_to_set_password').can_enable_at_pw_protection()</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="disable_password_protect" meta_type="CMF Action"
     i18n:domain="collective.folderprotection">
